      <Rectangle x:Name="Rect1" Width="10" Height="1" Fill="Black"/>
      <Rectangle x:Name="Rect2" Width="10" Height="1" Canvas.Top="6" Fill="Black"/>
      <Rectangle x:Name="Rect3" Width="10" Height="1" Canvas.Top="12" Fill="Black"/>
      <Rectangle x:Name="Rect4" Width="10" Height="1" Canvas.Top="18" Fill="Black"/>
      <Rectangle x:Name="Rect5" Width="10" Height="1" Canvas.Top="24" Fill="Black"/>
    </Canvas>
  </Viewbox>
</UserControl>
