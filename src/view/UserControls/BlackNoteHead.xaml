<UserControl x:Class="tunelint.view.UserControls.BlackNoteHead"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:tunelint.view.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="10" d:DesignWidth="10">
  <Viewbox Stretch="Uniform">
    <Canvas Name="NoteCanvas" 
            Width="10"
            Height="10">
      <Ellipse Name="NoteEllipse"
           Canvas.Left="0.65" 
           Width="8.7" 
           Canvas.Top="2.1" 
           Height="5.8"
           Fill="Black">
        <Ellipse.RenderTransform>
          <TransformGroup>
            <RotateTransform Angle="-33.33"
                             CenterX="4.35"
                             CenterY="2.9"/>
          </TransformGroup>
        </Ellipse.RenderTransform>
      </Ellipse>
    </Canvas>
  </Viewbox>
</UserControl>
